{
  "name": "@supernal/interface",
  "version": "1.0.13",
  "description": "Universal AI Interface - Make any application AI-controllable with decorators (Open Source Edition)",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/esm/src/browser.js",
      "types": "./dist/esm/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    },
    "./mcp-server": {
      "types": "./dist/esm/src/mcp-server.d.ts",
      "import": "./dist/esm/src/mcp-server.js",
      "require": "./dist/cjs/src/mcp-server.js"
    },
    "./browser": {
      "types": "./dist/esm/src/browser.d.ts",
      "import": "./dist/esm/src/browser.js",
      "require": "./dist/cjs/src/browser.js"
    },
    "./react": {
      "types": "./dist/esm/src/react/index.d.ts",
      "import": "./dist/esm/src/react/index.js",
      "require": "./dist/cjs/src/react/index.js"
    },
    "./testing": {
      "types": "./dist/esm/src/testing/index.d.ts",
      "import": "./dist/esm/src/testing/index.js",
      "require": "./dist/cjs/src/testing/index.js"
    },
    "./testing/graph-tester": {
      "types": "./dist/esm/src/testing/graph-tester/index.d.ts",
      "import": "./dist/esm/src/testing/graph-tester/index.js",
      "require": "./dist/cjs/src/testing/graph-tester/index.js"
    },
    "./testing/graph-tester/fixtures": {
      "types": "./dist/esm/src/testing/graph-tester/fixtures/index.d.ts",
      "import": "./dist/esm/src/testing/graph-tester/fixtures/index.js",
      "require": "./dist/cjs/src/testing/graph-tester/fixtures/index.js"
    },
    "./testing/ComponentStateHelpers": {
      "types": "./dist/esm/src/testing/ComponentStateHelpers.d.ts",
      "import": "./dist/esm/src/testing/ComponentStateHelpers.js",
      "require": "./dist/cjs/src/testing/ComponentStateHelpers.js"
    },
    "./adapters": {
      "types": "./dist/esm/src/adapters/index.d.ts",
      "import": "./dist/esm/src/adapters/index.js",
      "require": "./dist/cjs/src/adapters/index.js"
    },
    "./adapters/copilotkit": {
      "types": "./dist/esm/src/adapters/copilotkit/index.d.ts",
      "import": "./dist/esm/src/adapters/copilotkit/index.js",
      "require": "./dist/cjs/src/adapters/copilotkit/index.js"
    },
    "./storage": {
      "types": "./dist/esm/src/storage/index.d.ts",
      "import": "./dist/esm/src/storage/index.js",
      "require": "./dist/cjs/src/storage/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache",
    "build": "npm run clean && npm run build:esm && npm run fix-esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "fix-esm": "node scripts/fix-esm-imports.js",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "NODE_ENV=test npx jest",
    "test:watch": "NODE_ENV=test npx jest --watch",
    "test:coverage": "NODE_ENV=test npx jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepack": "npm run build"
  },
  "keywords": [
    "ai",
    "interface",
    "decorators",
    "tools",
    "testing",
    "automation",
    "chat-ui",
    "copilotkit",
    "adapter-pattern",
    "typescript"
  ],
  "author": "Supernal Intelligence",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ai-sdk/mcp": "^0.0.8",
    "cac": "^6.7.14",
    "glob": "^13.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.2"
  },
  "bin": {
    "si-oss": "./dist/cjs/src/cli/upgrade.js",
    "supernal-upgrade": "./dist/cjs/src/cli/upgrade.js"
  },
  "files": [
    "dist",
    "src/claude",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/supernalintelligence/supernal-interface.git",
    "directory": "packages/interface"
  },
  "homepage": "https://supernal.ai",
  "bugs": {
    "url": "https://github.com/supernalintelligence/supernal-interface/issues"
  }
}
