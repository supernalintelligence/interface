{
  "name": "@supernal/interface-nextjs",
  "version": "1.0.27",
  "description": "Next.js integration for Supernal Intelligence Interface - enables agentic UX with one line of code",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@supernal/interface": "*",
    "next": "^14.0.0 || ^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@supernal/interface": "file:../",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "nextjs",
    "ai",
    "chat",
    "navigation",
    "tools",
    "agentic",
    "ux"
  ],
  "author": "Supernal Intelligence",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supernalintelligence/supernal-interface.git",
    "directory": "packages/interface-nextjs"
  },
  "homepage": "https://supernal.ai",
  "bugs": {
    "url": "https://github.com/supernalintelligence/supernal-interface/issues"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-directive": "^3.0.0",
    "rehype-katex": "^7.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "mermaid": "^11.4.1",
    "katex": "^0.16.11"
  }
}
